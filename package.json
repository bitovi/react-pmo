{
  "name": "react-pmo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "eslint": "eslint vite.config.ts src",
    "prettier": "prettier --check vite.config.ts src",
    "depcheck": "depcheck .",
    "dev": "vite",
    "test": "vitest watch",
    "test:ci": "vitest run --coverage",
    "clean": "rm -rf tsconfig.tsbuildinfo node_modules/.vite dist coverage",
    "build": "vite build"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@bitovi/eslint-config/react"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@bitovi/eslint-config": "^1.7.0",
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.1",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.2.1"
  }
}
